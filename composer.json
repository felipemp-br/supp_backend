		"symfony/browser-kit": "7.1.*",
		"symfony/debug-bundle": "7.1.*",
		"symfony/maker-bundle": "1.57.0",
		"symfony/requirements-checker": "2.0.1",
		"symfony/stopwatch": "7.1.*",
		"symfony/var-dumper": "7.1.*",
		"symfony/web-profiler-bundle": "7.1.*"
	},
	"config": {
		"platform": {
			"php": "8.3.0"
		},
		"preferred-install": {
			"*": "dist"
		},
		"sort-packages": true,
		"allow-plugins": true
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/supp-core/elasticsearch-dsl.git"
		},
		{
			"type": "vcs",
			"url": "https://github.com/supp-core/elasticsearch-bundle.git"
		},
		{
			"type": "vcs",
			"url": "https://github.com/supp-core/PackageBase.git"
		},
		{
			"type": "vcs",
			"url": "https://github.com/supp-core/supp-consultivo-backend.git"
		},
		{
			"type": "vcs",
			"url": "https://github.com/supp-core/supp-disciplinar-backend.git"
		},
		{
			"type": "vcs",
			"url": "https://github.com/supp-core/supp-judicial-backend.git"
		},
		{
			"type": "vcs",
			"url": "https://github.com/supp-core/supp-ecarta-backend.git"
		},
		{
			"type": "vcs",
			"url": "https://github.com/supp-core/supp-receita-federal-backend.git"
		},
		{
			"type": "vcs",
			"url": "https://github.com/supp-core/supp-gestao-devedor-backend.git"
		}
	],
	"autoload": {
		"psr-4": {
			"SuppCore\\AdministrativoBackend\\": "src/"
		},
		"classmap": [],
		"exclude-from-classmap": []
	},
	"autoload-dev": {
		"psr-4": {
			"SuppCore\\AdministrativoBackend\\Tests\\": "tests/",
			"PHPUnit\\": "vendor-bin/phpunit/vendor/phpunit/phpunit/src/",
			"PHPUnit\\Framework\\MockObject\\": "vendor-bin/phpunit/vendor/phpunit/phpunit/src/Framework/MockObject",
			"Symfony\\Component\\BrowserKit\\": "vendor-bin/phpunit/vendor/symfony/browser-kit",
			"Symfony\\Component\\DomCrawler\\": "vendor-bin/phpunit/vendor/symfony/dom-crawler",
			"Symfony\\Bridge\\PhpUnit\\": "vendor-bin/phpunit/vendor/symfony/phpunit-bridge",
			"Liuggio\\Fastest\\": [
				"vendor-bin/phpunit/vendor/liuggio/fastest/src",
				"vendor-bin/phpunit/vendor/liuggio/fastest/adapters"
			],
			"Symplify\\CodingStandard\\": "vendor-bin/php-cs-fixer/vendor/symplify/easy-coding-standard/vendor/symplify/coding-standard/src",
			"Symplify\\EasyCodingStandard\\": "vendor-bin/php-cs-fixer/vendor/symplify/easy-coding-standard/src",
			"Symplify\\RuleDocGenerator\\": "vendor-bin/php-cs-fixer/vendor/symplify/easy-coding-standard/vendor/symplify/rule-doc-generator-contracts/src",
			"PHPStan\\PhpDoc\\PHPUnit\\": "vendor-bin/phpstan/vendor/phpstan/phpstan-phpunit/src/PhpDoc/PHPUnit",
			"PHPStan\\Rules\\PHPUnit\\": "vendor-bin/phpstan/vendor/phpstan/phpstan-phpunit/src/Rules/PHPUnit",
			"PHPStan\\Symfony\\": "vendor-bin/phpstan/vendor/phpstan/phpstan-symfony/src/Symfony",
			"PHPStan\\Type\\PHPUnit\\": "vendor-bin/phpstan/vendor/phpstan/phpstan-phpunit/src/Type/PHPUnit",
			"PHPStan\\Type\\Symfony\\": "vendor-bin/phpstan/vendor/phpstan/phpstan-symfony/src/Type/Symfony",
			"Rector\\Core\\": "vendor-bin/rector/vendor/rector/rector/src",
			"Rector\\Php74\\": "vendor-bin/rector/vendor/rector/rector/rules/Php74",
			"Rector\\Php80\\": "vendor-bin/rector/vendor/rector/rector/rules/Php80",
			"Rector\\Set\\": "vendor-bin/rector/vendor/rector/rector/packages/Set"
		}
	},
	"replace": {
		"symfony/polyfill-ctype": "*",
		"symfony/polyfill-mbstring": "*",
		"symfony/polyfill-php72": "*",
		"symfony/polyfill-php80": "*",
		"symfony/polyfill-php81": "*",
		"symfony/polyfill-php82": "*",
		"symfony/polyfill-php83": "*"
	},
	"scripts": {
		"auto-scripts": {
			"cache:clear": "symfony-cmd",
			"cache:warmup": "symfony-cmd",
			"assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
			"assets:install %PUBLIC_DIR%": "symfony-cmd",
			"requirements-checker": "script"
		},
		"post-install-cmd": [
			"rm -rf src/DataFixtures/AppFixtures.php",
			"if test -d vendor/symfony/requirements-checker; then ./vendor/bin/requirements-checker; fi",
			"if test -d vendor/bamarni/composer-bin-plugin; then composer bin all install --optimize-autoloader; fi",
			"@auto-scripts",
			"@composer dump-autoload"
		],
		"post-update-cmd": [
			"rm -rf src/DataFixtures/AppFixtures.php",
			"if test -d vendor/symfony/requirements-checker; then ./vendor/bin/requirements-checker; fi",
			"if test -d vendor/bamarni/composer-bin-plugin; then composer bin all update --optimize-autoloader; fi",
			"@auto-scripts",
			"@composer dump-autoload"
		]
	},
	"conflict": {
		"symfony/symfony": "*",
		"symfony/twig-bundle": "<3.3",
		"symfony/debug": "<3.3"
	},
	"extra": {
		"allow-contrib": "true",
		"projectTitle": "Sistema Único de Procuradorias Públicas",
		"bamarni-bin": {
			"bin-links": true,
			"forward-command": true,
			"target-directory": "vendor-bin"
		},
		"symfony": {
			"require": "7.1.*"
		}
	}
}
